{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "../app",
    "paths": {
      "@/*": ["*"]
    },
    "rootDir": "../app",
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "pretty": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2015", "dom"]
  },
  "exclude": []
}
